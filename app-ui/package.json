{
  "name": "spidertracks-test-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.2",
    "bootstrap": "^5.1.3",
    "mdbreact": "^5.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-query": "^3.39.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/mocks/**/*.{js,jsx}",
      "!src/lib/**/*.{js,jsx}",
      "!**/constants/**/*.{js,jsx}",
      "!src/config/**/*.{js,jsx}",
      "!src/stores/**/*.{js,jsx}",
      "!src/utils/**/*.{js,jsx}",
      "!src/services/**/*.{js,jsx}",
      "!src/components/**/*.{js,jsx}",
      "!src/views/app/**/*.{js,jsx}",
      "!src/index.js",
      "!src/config.js",
      "!src/reportWebVitals.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@zarconontol/enzyme-adapter-react-18": "^0.7.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.4"
  }
}